{
  "name": "make_report",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    },
    "copy:views": {
      "patterns": [
        "src"
      ],
      "extensions": "ejs,js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && babel src -d dist",
    "copy:views": "rimraf dist/views && cross-env COPYFILE_DISABLE=true (cp -R src/views dist/views || xcopy /I /S /Y src\\views\\ dist\\views\\",
    "dev": "concurrently \"npm run build && npm run copy:views && nodemon --watch src --watch views dist/index.js\" \"npm run watch\" ",
    "start": "node dist/index.js",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mampionona33/make_report.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mampionona33/make_report/issues"
  },
  "homepage": "https://github.com/Mampionona33/make_report#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "babel-watch": "^7.7.2",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "npm-watch": "^0.11.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "format-duration": "^3.0.2",
    "mongoose": "^7.0.3",
    "multer": "^1.4.5-lts.1"
  }
}
